/dts-v1/;

#include "msm8939.dtsi"
#include "pm8916.dtsi"
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>
#include <dt-bindings/interrupt-controller/irq.h>
#include <dt-bindings/leds/common.h>
#include <dt-bindings/pinctrl/qcom,pmic-gpio.h>
#include <dt-bindings/sound/apq8016-lpass.h>

/ {
	model = "Huawei MediaPad T2 Pro 10";
	compatible = "huawei,federer", "qcom,msm8939";
	chassis-type = "tablet";
	qcom,msm-id = <239 0>;
	qcom,board-id =	<8000 0>, <8000 4>, // t2_a01
                    <8016 0>, <8016 4>,
                    <8032 0>, <8032 4>,
                    <8112 0>, <8112 4>,
                    <8128 0>, <8128 4>;
	// huawei,modem_id = <0xFFF40096 0x0>;

	aliases {
		serial0 = &blsp1_uart2;
	};

	chosen {
		stdout-path = "serial0:115200n8";
	};

	reserved-memory {
		/delete-node/ venus@90000000;
		// /delete-node/ rmtfs@86681000;
		/delete-node/ rfsa@867e0000;
		/delete-node/ mba@8ea00000;

		cont_splash_mem: framebuffer@83000000 {
			compatible = "removed-dma-pool";
			reg = <0x0 0x83000000 0x0 0x2000000>;
			no-map;
		};

		reserved@85000000 {
			reg = <0x0 0x85000000 0x0 0x1000000>;
			no-map;
		};

		/*rmtfs@86700000 {
			compatible = "qcom,rmtfs-mem";
			//reg = <0x0 0x86690000 0x0 0x100000>;
			reg = <0x0 0x86700000 0x0 0x200000>;
			no-map;
			qcom,client-id = <1>;
		};*/

		gps_mem: gps@8c400000 {
			status = "disabled";
			reg = <0x0 0x8c400000 0x0 0x200000>;
			no-map;
		};

		tz-apps@8c700000 {
			reg = <0x0 0x8c700000 0x0 0x400000>;
			no-map;
		};

		ramoops: ramoops_mem_region@8cb00000 {
			compatible = "ramoops";
			reg = <0x0 0x8cb00000 0x0 0x200000>;

			record-size = <65536>;
			console-size = <1048576>;
			ftrace-size = <0>;
		};

		venus_mem: venus@8f800000 {
			reg = <0x0 0x8f800000 0x0 0x800000>;
			no-map;
		};

		reserved@f7800000 {
			reg = <0x0 0xf7800000 0x0 0x8400000>;
			no-map;
		};

		mba_mem: mba@ffc00000 {
			reg = <0 0xffc00000 0 0x400000>;
			no-map;
		};
	};

	/*huawei_fac_info {
		compatible = "huawei,hw_fac_info";
		fac,product_name = "HUAWEI FDR-A01L";
		fac,hardware_ver = "SH1FDRA01LM_VERA";
		fac,software_ver = "MSM8939_FDR_A01L_VA";
		status = "ok";
	};*/

	bat: battery {
		compatible = "simple-battery";
		voltage-min-design-microvolt = <3400000>;
		voltage-max-design-microvolt = <4350000>;
		energy-full-design-microwatt-hours = <24700000>;
		charge-full-design-microamp-hours = <6500000>;

        precharge-current-microamp = <1300000>;
        charge-term-current-microamp = <260000>;

		ocv-capacity-celsius = <25>;
		ocv-capacity-table-0 = <4314000 100>, <4252000 95>, <4197000 90>,
					<4143000 85>, <4092000 80>, <4044000 75>, <3999000 70>,
					<3953000 65>, <3905000 60>, <3868000 55>, <3841000 50>,
					<3818000 45>, <3799000 40>, <3783000 35>, <3767000 30>,
					<3748000 25>, <3724000 20>, <3699000 16>, <3678000 13>,
					<3671000 11>, <3668000 10>, <3664000 9>, <3659000 8>,
					<3649000 7>, <3627000 6>, <3588000 5>, <3535000 4>,
					<3469000 3>, <3381000 2>, <3252000 1>, <3000000 0>;
	};

	gpio-hall-sensor {
		compatible = "gpio-keys";

		pinctrl-names = "default";
		pinctrl-0 = <&gpio_hall_sensor_default>;

		label = "GPIO Hall Effect Sensor";

		hall-sensor {
			label = "Hall Effect Sensor";
			gpios = <&msmgpio 69 GPIO_ACTIVE_LOW>;
			linux,input-type = <EV_SW>;
			linux,code = <SW_LID>;
			linux,can-disable;
		};
	};

	gpio-keys {
		compatible = "gpio-keys";

		pinctrl-names = "default";

		label = "GPIO Buttons";

		volume-up {
			label = "Volume Up";
			gpios = <&msmgpio 107 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_VOLUMEUP>;
		};
	};

	leds {
		compatible = "gpio-leds";

		pinctrl-names = "default";
		pinctrl-0 = <&gpio_leds_default>;

		led-0 {
			gpios = <&msmgpio 8 GPIO_ACTIVE_HIGH>;
			label = "red";
			default-state = "off";
			retain-state-suspended;
		};

		led-1 {
			gpios = <&msmgpio 9 GPIO_ACTIVE_HIGH>;
			label = "green";
			default-state = "off";
			retain-state-suspended;
		};

		led-2 {
			gpios = <&msmgpio 10 GPIO_ACTIVE_HIGH>;
			label = "blue";
			default-state = "off";
			retain-state-suspended;
		};
	};
	
	lcd_pp3v3: regulator-pp3v3 {
		compatible = "regulator-fixed";
		regulator-name = "lcd_pp3v3";
		vin-supply = <&pm8916_l17>;
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		regulator-always-on;
		regulator-boot-on;
	};
	
	lcd_backlight: gpio-regulator {
		compatible = "regulator-gpio";
		regulator-name = "lcd_backlight";
		enable-active-high;
		regulator-boot-on;
		gpios = <&msmgpio 109 GPIO_ACTIVE_HIGH>;
		startup-delay-us = <5000000>;
		//pinctrl-names = "default";
		//pinctrl-0 = <&lcd_backlight_default>;
	};

	lcd_pp1v8: regulator-lcd-pp1v8 {
		compatible = "regulator-fixed";
		regulator-name = "lcd_pp1v8";
		vin-supply = <&pm8916_l6>;
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		regulator-always-on;
		regulator-boot-on;
	};

	lcd_iovcc: gpio-regulator-iovcc {
		compatible = "regulator-gpio";
		regulator-name = "lcd_iovcc";
		enable-active-high;
		regulator-boot-on;
		gpios = <&msmgpio 32 GPIO_ACTIVE_HIGH>;
		startup-delay-us = <200000>;
		vin-supply = <&lcd_pp1v8>;
		
		pinctrl-names = "default";
		pinctrl-0 = <&lcd_panel_default>;
	};

	reg_lcd_vsn: regulator-lcd-vsn {
		compatible = "regulator-fixed";
		regulator-name = "lcd_vsn";
		regulator-min-microvolt = <5400000>;
		regulator-max-microvolt = <5400000>;
		regulator-always-on;
		regulator-boot-on; // allows fb console to show up briefly at boot without always-on
		/*gpio = <&msmgpio 32 GPIO_ACTIVE_HIGH>;
		startup-delay-us = <200000>; //delay 200
		enable-active-high;
		pinctrl-names = "default";
		pinctrl-0 = <&lcd_vsn_default>;*/
	};

	reg_lcd_vsp: regulator-lcd-vsp {
		compatible = "regulator-fixed";
		regulator-name = "lcd_vsp";
		regulator-min-microvolt = <5400000>;
		regulator-max-microvolt = <5400000>;
		regulator-always-on;
		regulator-boot-on; // allows fb console to show up briefly at boot without always-on
		/*gpio = <&msmgpio 97 GPIO_ACTIVE_HIGH>;
		enable-active-high;
		startup-delay-us = <200>; //delay 200
		pinctrl-names = "default";
		pinctrl-0 = <&lcd_vsp_default>;*/
	};

	lcd_vled_vsp: gpio-regulator-vsp {
		compatible = "regulator-gpio";
		regulator-name = "vsp";
		enable-active-high;
		regulator-boot-on;
		gpios = <&msmgpio 97 GPIO_ACTIVE_HIGH>;
		startup-delay-us = <200000>;
		vin-supply = <&reg_lcd_vsp>;
		pinctrl-names = "default";
		pinctrl-0 = <&lcd_vled_default>;
	};

	lcd_vled_vsn: gpio-regulator-vsn {
		compatible = "regulator-gpio";
		regulator-name = "vsn";
		enable-active-high;
		regulator-boot-on;
		gpios = <&msmgpio 97 GPIO_ACTIVE_HIGH>;
		startup-delay-us = <200000>;
		vin-supply = <&reg_lcd_vsn>;
		pinctrl-names = "default";
		pinctrl-0 = <&lcd_vled_default>;
	};

